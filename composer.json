{
	"name": "janrain/plex",
	"type": "library",
	"license": "Apache-2.0",
	"version": "0.1.0",
	"minimum-stability": "dev",
	"require": {
		"php": ">5.3.0",
		"psr/log": "*"
	},
	"autoload": {
		"psr-0": {
			"janrain": "src/"
		}
	},
	"require-dev": {
		"php": ">=5.5.0",
		"phpunit/phpunit": "*",
		"sami/sami": "*"
	},
	"scripts": {
		"post-update-cmd": [
			"./vendor/bin/phpunit --coverage-html coverage",
			"./vendor/bin/sami.php update ./sami_config.php"
		]
	}
}

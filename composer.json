{
	"name": "janrain/plex",
	"type": "library",
	"license": "Apache-2.0",
	"version": "0.1.0",
	"minimum-stability": "dev",
	"require": {
		"php": ">5.3.0",
		"psr/log": "*"
	},
	"autoload": {
		"psr-0": {
			"janrain": "lib/"
		}
	},
	"require-dev": {
		"php": ">=5.5.0",
		"phpunit/phpunit": "*",
		"apigen/apigen": "2.8.0"
	},
	"scripts": {
		"post-install-cmd": [
			"apigen.php  --source ./lib --destination docs --php no",
			"phpunit --coverage-html coverage"
		],
		"post-update-cmd": [
			"phpunit --coverage-html coverage"
		]
	}
}
